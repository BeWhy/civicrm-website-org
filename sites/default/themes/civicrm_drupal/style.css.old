
* {
	margin: 0;
	padding: 0;
}

a {
	color: #9b3554;
}

blockquote {
	text-align: center;
	font-weight: bold;
	font-style: oblique;
	margin: 15px 0;
}

blockquote span {
	text-transform: uppercase;
	font-style: normal;
	margin: 10px;
}

body {
	background-color: #fff;
	font-size: 10pt;
	font-family: "Verdana", "Arial", sans-serif;
	line-height: 145%;
	color: #636363;
	height: 100%;
}

h2 {
	color: #079094;
	font-weight: normal;
	margin-bottom: 5px; 
}

h3 {
	color: #f33969;
	font-size: 95%;
	text-transform: uppercase;
	letter-spacing: 1px;
	margin: 5px 0;

}

html {
	height: 100%;
}

hr {
	display: hidden;
	height: 20px;
	color: white;
	border: 1px solid white;
}

iframe {
	border: 0;
	width: 450px;
	height: 500px;
}

li {
	list-style: none;
}

p {
	margin: 5px 0;
}

table {
	line-height: normal;
	font-size: 90%;
}

td {
	padding: 3px 10px;
}

th {
	background-color: gray;
	color: white;
	padding: 2px 5px;
}

tr {
	vertical-align: top;
	border-bottom: 1px solid #eee;
}

.breadcrumb {
	float: right;
}

.content ul li {
	margin-left: 25px;
	list-style-image: url(/sites/civicrm.org/images/bullet_green_small.gif);
	margin-bottom: 10px;
}

.content ol {
	margin-bottom: 10px;
}

.content ol li {
	margin-left: 25px;
	list-style-type: decimal;
}





.content dl {
        margin-left: 25px;
        margin-bottom: 10px;
}

.content dl dt {
	font-weight: bold;
}

.content dl dd {
	margin-bottom: 5px;
}



.fp_box {
	width: 45%;
	line-height: 130%;
	padding: 2% 0;
	float: left;
	color: black;
	font-size: 90%;
	line-height: normal;
	display: none;
}

.fp_box h2 {
	display: none;
}
.fp_box dt {
	font-size: 145%;
	color: #079094;
	margin-bottom: 10px;
	border-bottom: 1px solid #ccc;
}

.info {
	font-size: 75%;
	margin-bottom: 10px;
//	display: none;
}

.links {
	text-transform: capitalize;
	margin-bottom: 25px;
}

.node-form .standard {
  clear: none !important;
}

.messages {
	background-color: #feffb8;
	color: black; 
	padding: 2px 4px;
	margin-bottom: 10px;
}

.profile {
	clear: none !important;
}

.quotes-quote {
	font-style: italic;
}

#banner {
	clear: both;
	float: left;
	width: 84%;
	height: 35px;
	padding-left: 8%;
	padding-right: 8%;
	padding-top: 15px;
	margin-top: 3px;
	background: url(/sites/civicrm.org/images/aqua_grad.gif) bottom left repeat-x;
	color: #f33969;
	line-height: 110%;
}

#banner h1 {
	color: #167082;
	font-weight: normal;
	font-family: "arial";
	font-size: 150%;
	line-height: normal;
}

#banner p {
	display: none;
	line-height: 125%;
}

#banner img { behavior: url(/sites/civicrm.org/images/iepngfix.htc) }


#box {
	width: 90%;
	// float: left;
	margin-bottom: 10px;
	border: 1px solid #bcbcbc;
	background-color: white;
	padding: 10px;
	color: black;
	font-size: 90%;
	height: 60px;
	-moz-border-radius: 5px;
}

#box h3 {
	margin-top: 10px;
	color: black;
	text-transform: none;
	font-size: 130%;
	margin-left: 7px;
	border-bottom: none;
	letter-spacing: normal;
	font-weight: normal;
	margin-bottom: 0;
	padding-bottom: 0;
	clear: none;
}

#box h3 a {
	color: black;
}

#box img {
	float: left;
}

#box li {
	list-style-image: none;
	margin: 0;
	padding: 0;
}

#box p {
	margin-left: 7px;
	margin-top: 0;
	color: #167082;
	font-size: 110%;
}

#box strong {
	font-size: 125%;
}

.introbox {
	background: url(/sites/civicrm.org/images/yellow_grad.gif) repeat-x;
	color: black;
	font-size: 150% !important;
	padding: 50px;
}

#column_left {
	margin-right: 250px;
}

#column_left ul {
	font-size: 95%;
	list-style-type: circle;
}

#column_right {
	float: right;
	width: 200px;
	padding-top: 0;
}

#content {
	padding-left: 5%;
	padding-right: 5%;
	margin-left: auto;
	margin-right: auto;
	background: url(/sites/civicrm.org/images/blue_grad.gif) repeat-x;
	clear: both;
}

#content h1 {
	color: #59c543;
	font-size: 300%; 
	font-weight: normal;
	border-left: 4px solid #f33969;
	padding-left: 40px;
	min-height: 40px;
	padding-top: 0px;
	padding-bottom: 10px;
	line-height: normal;
}

#content h1:first-word {
	color: 167082;
}

#content_type {
	background-color: white;
	padding: 25px;
	clear: both;
}

#demo {
	margin-top: -35px;
	float: right;
}

#download {
	background-color: #8dd035;
	border: 1px solid #82b541;
	padding: 8px;
	height: 25px;
	width: 240px;
	text-align: center;
	display: none;
}

#download:hover {
	background-color: #079094;
}

#download h1 {
	color: white;
	margin: 0;
	float: left;
	padding-right: 10px;
}

#download h1 a {
	color: white;
	text-decoration: none;
}

#download li {
	list-style: none;
	display: block;
}

#download small {
	color: #000;
	font-size: 70%
	line-height: 80%;
}

#footer {
	margin-top: 35px;
	border-top: 1px solid #ccc;
	padding: 25px;
	padding-bottom: 5px;
	clear: both;
	background-color: #8fcbd8;
	color: black;
}

#footer a {
	color: black;
}

#footer p {
	margin: 0;
	padding: 0;
}

.footer-menu {
	text-align: center;
	margin-bottom: 10px;
}
.footer-menu li {
	display: inline;
	margin-right: 5px;
}

#footer small {
	text-align: center;
}

#header {
	background: url(/sites/civicrm.org/images/header_bg.gif) top right repeat-x;
	border-bottom: 4px solid #c1e0e5;
	float: left;
	width: 100%;
}

#header img {
	float: left;
	padding: 0;
}

#logo {
	float: right !important;
	display: none;
}

#primary {
	margin-left: 20px;
}

#primary a {
	color: white;
	padding: 16px 20px 15px 20px;
	float: left;
	background-color: #9b1240;
	border-left: 1px solid #b5466b;
	text-decoration: none;
	font-size: 110%;
	vertical-align: middle;
}

#primary a:hover, #primary a.active {
	background-color: #23a1bd;
}

#primary li {
	display: inline;
	list-style: none;
}

#nav_secondary {
	margin-bottom: 35px;
}

#nav_secondary a {
	color: #9b3554;
	font-size: 110%;
}

#nav_secondary a:hover, #nav_secondary a.active {
	color: #59c543;
}

#nav_secondary dt a.active {
	color: #f33969;
	font-weight: normal;
	}	

#nav_secondary a.active {
	font-weight: bold;
}

#nav_secondary dd {
	padding-left: 35px;
	margin-bottom: 10px;
}

#nav_secondary dt {
	color: #f33969;
	height: 30px;
	font-weight: normal;
	margin-top: 40px;
	margin-bottom: 10px;
	font-family: "arial";
	font-size: 230%;
	padding-left: 35px;
	background: url(/sites/civicrm.org/images/bullet_green.gif) top left no-repeat;
}

#nav_secondary dt a {
	text-decoration: none;
}

#nav_secondary dt span {
	color: #167082;
}

#util {
	float: right;
	margin: 3px 15px 0 0;
	font-size: 75%;
}

#util li {
	list-style: none;
	display: inline;
}

#util a {
	color: white;
	text-decoration: none;
	padding-left: 4px;
	padding-right: 4px;
	border-left: 1px solid white;
}

#util a:hover {
	text-decoration: underline;
}

#wrapper {
	position: relative;
}

